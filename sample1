      {% if end_time and end_time != '' -%}
      {{ end_time | regex_replace('\\s+', ' ') | trim }}
      {%- else -%}
      {{ (normalized_start | to_datetime('%Y-%m-%d %H:%M') | ansible.builtin.strftime('%Y-%m-%d %H:') + '%02d' | format(((normalized_start | to_datetime('%Y-%m-%d %H:%M')).hour + 1) % 24)) }}
      {%- endif %}
