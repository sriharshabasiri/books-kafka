and(
    equals(item()?['Day'], formatDateTime(utcNow(), 'dddd')),
    greaterOrEquals(
        div(sub(ticks(utcNow()), ticks('1899-12-30T00:00:00Z')), 864000000000),
        float(item()?['FromTime'])
    ),
    lessOrEquals(
        div(sub(ticks(utcNow()), ticks('1899-12-30T00:00:00Z')), 864000000000),
        float(item()?['ToTime'])
    )
)
