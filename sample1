    normalized_end: >-
      {%- if end_time and end_time != '' -%}
      {{ end_time | trim }}
      {%- else -%}
      {%- set ns = namespace() -%}
      {%- set ns.start = (normalized_start | to_datetime('%Y-%m-%d %H:%M')) -%}
      {%- set ns.end = ns.start + namespace.datetime.timedelta(hours=1) -%}
      {{ ns.end.strftime('%Y-%m-%d %H:%M') }}
      {%- endif %}
